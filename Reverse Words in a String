class Solution {
    public String reverseWords(String s) {
        s=s.replaceAll("( )+"," ");
        s.trim();
        String arr[]=s.split(" ");
        String reversed="";
        for(int i=arr.length-1;i>=0;i--){
            reversed=reversed+arr[i]+" ";
        }
        return reversed.trim();
    }
}
